<footer class="covidtest-footer">
  <div class="covidtest-footer__container covidtest-container">
    <div class="covidtest-footer__logos">
      <a class="covidtest-footer__logo-link" href="https://www.hhs.gov/" class="margin-right-4" rel="noopener">
        <img class="covidtest-footer__logo-img" src="images/covidtest/hhs-logo.png" alt="<%= t('shared.footer.links.hhs') %>" width="146" height="151" loading="lazy">
      </a>
      <a class="covidtest-footer__logo-link" href="https://www.usps.com/" rel="noreferrer noopener">
        <img class="covidtest-footer__logo-img" src="images/covidtest/usps-logo.svg" alt="<%= t('shared.footer.links.usps') %>" width="210" height="25" loading="lazy">
      </a>
      <a class="covidtest-footer__logo-link" href="https://wecandothis.hhs.gov/" rel="noopener">
        <img class="covidtest-footer__logo-img" src="images/covidtest/wecandothis-logo.svg" alt="<%= t('shared.footer.links.covid') %>" width="155" height="155" loading="lazy">
      </a>
      <a class="covidtest-footer__logo-link covidtest-footer__logo-text" href="/">
        <%= t('shared.header.title') %>
      </a>
    </div>
  </div>
</footer>
